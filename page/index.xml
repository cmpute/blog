&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="nx">aliases&lt;/span> &lt;span class="p">=&lt;/span> &lt;span class="p">[&lt;/span>&lt;span class="s2">&amp;#34;some-page-link&amp;#34;&lt;/span>&lt;span class="p">,&lt;/span> &lt;span class="s2">&amp;#34;AnOtHER_LinK&amp;#34;&lt;/span>&lt;span class="p">]&lt;/span> &lt;span class="c"># 给当前页面添加别的路径&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="nx">license&lt;/span> &lt;span class="p">=&lt;/span> &lt;span class="s2">&amp;#34;CC BY-NC-ND&amp;#34;&lt;/span> &lt;span class="c"># 指定当前页面的分享协议&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="p">[&lt;/span>&lt;span class="nx">menu&lt;/span>&lt;span class="p">.&lt;/span>&lt;span class="nx">main&lt;/span>&lt;span class="p">]&lt;/span> &lt;span class="c"># 在左侧菜单添加当前页面的入口&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="nx">weight&lt;/span> &lt;span class="p">=&lt;/span> &lt;span class="mi">-90&lt;/span> &lt;span class="c"># 用于排序&lt;/span>
